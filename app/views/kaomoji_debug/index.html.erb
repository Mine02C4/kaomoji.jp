<h1>顔文字テーブル</h1>
<table>
  <thead>
    <tr>
      <th>顔文字</th>
      <th>登録日時</th>
      <th>公開日時</th>
      <th>状態</th>
      <th>台詞の有無</th>
      <th></th>
      <th></th>
      <th></th>
    </tr>
  </thead>
  <tbody>
    <% @kaomojies.each do |kaomoji| %>
    <tr>
      <td><%= kaomoji.kaomoji %></td>
      <td><%= kaomoji.registered_time %></td>
      <td><%= kaomoji.published_time %></td>
      <td><%= kaomoji.status %></td>
      <td><%= kaomoji.has_words %></td>
      <td><%= link_to '表示', :action => 'show', :id => kaomoji.id %></td>
      <td><%= link_to '編集', edit_kaomoji_debug_path(kaomoji) %></td>
      <td><%= link_to '削除', {:action => 'destroy', :id => kaomoji.id}, method: :delete, data: { confirm: 'Are you sure?' } %></td>
    </tr>
    <% end %>
   </tbody>
</table>
<%= link_to '新しい顔文字', new_kaomoji_debug_path %>
